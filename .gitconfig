[user]
  email = delete44+tech@protonmail.com
  name = delete-44
[push]
  default = current
[rerere]
  enabled = 1
[credential]
  helper = osxkeychain
[safe]
  directory = /workspace
[init]
  defaultBranch = main
[core]
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  line-numbers = true
  side-by-side = true
  features = arctic-fox
[include]
  path = ./delta/themes.gitconfig
